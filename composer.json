    "keywords": ["iterator", "range", "interval", "coroutine", "helper", "functional"],
    "support": {
        "issues": "https://github.com/SerafimArts/Interval/issues",
        "source": "https://github.com/SerafimArts/Interval"
    },
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "require": {
